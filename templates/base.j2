<!DOCTYPE html>
<html class="dark" data-bs-theme="dark">
{% block head %}
<head>
<link rel="shortcut icon" href="{{ url_for('static', filename='favicon.ico') }}">
<meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
<meta http-equiv="Pragma" content="no-cache" />
<meta http-equiv="Expires" content="0" />
<link rel="stylesheet" href="https://cdn.datatables.net/2.0.3/css/dataTables.dataTables.css">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<link rel="stylesheet" href={{ url_for('static', filename = 'css/dnd.css') }}>

<script src="https://code.jquery.com/jquery-3.7.1.js"></script> 
<script src="https://cdn.datatables.net/2.0.3/js/dataTables.js"></script> 
<script src="https://cdn.datatables.net/plug-ins/2.0.3/sorting/natural.js"></script> 
<script src={{ url_for('static', filename = 'scripts/sets.js') }}></script>
</head>
{% endblock %}
     <div style="overflow: hidden; background-color: #333" class="topnav">
	     <!--<a href="{{ url_for('home') }}">Home Page</a>-->
             <a href="{{ url_for('index') }}">All users</a>
             <a href="{{ url_for('search') }}">Search</a>
     	     <a style="float: right;" href="https://buymeacoffee.com/satchemonbot"><img alt="Buy Me A Coffee" class="bullet" src= {{ url_for('static', filename = 'css/images/bmc-button.png') }} height="35"></a>
	     {% if current_user.is_authenticated %}
	       <a href="{{ url_for('mycards') }}">My Cards</a>
	       <a style="float: right;" href="{{ url_for('logout') }}"><img alt="Sign Out" class="bullet" src= {{ url_for('static', filename = 'css/images/Sign-out.png') }} height="35"></a>
	       <p style="float: right; align-items: center;"> Signed in as: {{ session.user }}</p>
	     {% else %}
	       <a style="float: right;" href="{{ url_for('discord_login') }}"><img alt="Sign In" class="bullet" src= {{ url_for('static', filename = 'css/images/Sign-in.png') }} height="35"></a>
	     {% endif %}
     </div>
<body>
   
	<div id="content">{% block content %}{% endblock %}</div>
</body>
</html>
